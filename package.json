{
  "name": "mcp-server-manager",
  "displayName": "MCP Server Manager",
  "description": "Model Context Protocol (MCP) Server Manager for Podman Desktop",
  "version": "0.0.1",
  "icon": "resources/icon.png",
  "publisher": "sparesparrow",
  "license": "Apache-2.0",
  "engines": {
    "podman-desktop": ">=1.10.0"
  },
  "main": "./dist/extension.js",
  "scripts": {
    "build": "tsc --noEmitOnError false && vite build",
    "build:extension": "tsc --noEmitOnError false",
    "build:frontend": "vite build",
    "watch": "vite",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "contributes": {
    "views": {
      "podman-desktop-view": [
        {
          "id": "mcp-server-manager",
          "name": "MCP Server Manager",
          "when": "true"
        }
      ]
    }
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "6.5.1",
    "@fortawesome/free-brands-svg-icons": "6.5.1",
    "@fortawesome/free-regular-svg-icons": "6.5.1",
    "@fortawesome/free-solid-svg-icons": "6.5.1",
    "@podman-desktop/api": "1.16.1",
    "@sveltejs/vite-plugin-svelte": "3.0.1",
    "@testing-library/svelte": "5.2.1",
    "@tsconfig/svelte": "5.0.2",
    "@types/node": "20.11.16",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "autoprefixer": "10.4.17",
    "eslint": "8.56.0",
    "postcss": "8.4.33",
    "postcss-import": "16.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "svelte": "4.2.9",
    "svelte-check": "3.6.3",
    "svelte-preprocess": "5.1.3",
    "tailwindcss": "3.4.1",
    "typescript": "5.3.3",
    "vite": "5.0.12",
    "vitest": "1.2.2"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "1.4.1"
  },
  "packageManager": "pnpm@8.15.1",
  "type": "module"
}
